<%
var homefeedData = public.content.homefeed._data.pins;
// var homefeedData = public.content.homefeed._data.ugly;
// var homefeedData = public.content.homefeed._data.real;
%>

<% for ( var i=0; i < homefeedData.length; i++ ) { %>
	<div class="main-card-wrapper cu-<%= i+1 %>">
		<img src="<%= homefeedData[i].img %>" />
		<div class="cu-pin-title"><%= homefeedData[i].title %></div>
		<div class="cu-pin-description">Pin description goes here</div>
		<div class="cu-pin-actions-wrap">
			<div class="leading-button-wrap">
				<div class="icon-button share"></div>
				<div class="icon-button comment"></div>
			</div>

			<div class="main-card-button-wrap">
				<div class="button secondary">Visit</div>
				<div data-id=<%= i+1 %> class="save-btn heart-<%= i+1 %>" id="heart-<%= i+1 %>"></div>
			</div>
		</div>
	</div>
<% } %>
